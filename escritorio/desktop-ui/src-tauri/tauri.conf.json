{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "CevicheriaSOS",
  "version": "0.1.0",
  "identifier": "cl.soscevicheria.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "SOS Cevicher√≠a & Sushi",
        "width": 1000,
        "height": 700,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
  "active": true,
  "targets": "all",
  "icon": [
    "icons/32x32.png",
    "icons/128x128.png",
    "icons/128x128@2x.png",
    "icons/icon.icns",
    "icons/icon.ico"
  ]
},
  "plugins": {
    "notification": {
      "permissions": ["*"]
    }
  }
}